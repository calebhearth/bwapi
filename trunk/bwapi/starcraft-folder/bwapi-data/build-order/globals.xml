<build-orders xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="build-order.xsd">
  <build-order type="function" name="Standard terran scv building">
    <train building="Terran Command Center" priority="10">
      <build name="Terran SCV" weight="1">
        <condition type="and">
          <condition type="formula" formula="ActiveMinerals*2 + All(Terran Refinery)*3 + InProgress(Terran Command Center)*10 + All(Terran Command Center)*3 - All(Terran SCV)" />
          <condition type="formula" formula="ConfigValue(max_count_of_workers) - All(Terran SCV)" />
        </condition>
      </build>
    </train>
  </build-order>
  <build-order type="function" name="Standard terran supply depot building">
    <condition type="and">
      <condition type="formula" formula="UsedSupply(Terran) + Factories(Terran)*4  - PlannedSupply(Terran)" />
      <condition type="formula" formula="200 - PlannedSupply(Terran)" />
    </condition>
    <build name="Terran Supply Depot" place="non-producting-3X2"/>
  </build-order>
  <build-order type="function" name="Standard protoss probe building">
    <train building="Protoss Nexus" priority="10">
      <build name="Protoss Probe" weight="1">
        <condition type="and">
          <condition type="formula" formula="ActiveMinerals*2 + All(Protoss Assimilator)*3 + InProgress(Protoss Nexus)*10 + All(Protoss Nexus)*3 - All(Protoss Probe)" />
          <condition type="formula" formula="ConfigValue(max_count_of_workers) - All(Protoss Probe)" />
        </condition>
      </build>
    </train>
  </build-order>  
</build-orders>